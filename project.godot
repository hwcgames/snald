[application]

config/name="Several Nights at Lucas's Domicile"
run/main_scene="res://scenes/menu/menu.tscn"
boot_splash/image="res://textures/doors-bg.png"
config/icon="res://icon.png"

[autoload]

LevelLoader="*res://scripts/level_loader.gd"
EventMan="*res://scripts/event_man.gd"
SpeedrunTimer="*res://eggs/speedrun_timer.gd"
PersistMan="*res://scripts/persist_man.gd"
Console="*res://addons/Console/Console.gd"
DebugCommands="*res://scripts/debug_commands.gd"
Modloader="*res://scripts/modloader.gd"
KeyPersistence="*res://scripts/KeyPersistence.gd"
CutsceneMan="*res://scripts/CutsceneMan.gd"
CVars="*res://scripts/CVars.gd"
MapQuery="*res://scripts/mapquery.gd"

[compression]

formats/zstd/compression_level=9
formats/zlib/compression_level=9
formats/gzip/compression_level=9

[debug]

settings/crash_handler/message="Please file an issue with this log at https://github.com/hwcgames/snald/issues"

[display]

window/size/width=640
window/size/height=480
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/qodot/plugin.cfg" )

[global]

sync=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 1,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

