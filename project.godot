[application]

config/name="Several Nights at Lucas's Domicile"
run/main_scene="res://scenes/menu/menu.tscn"
config/icon="res://icon.png"

[autoload]

LevelLoader="*res://scripts/level_loader.gd"
EventMan="*res://scripts/event_man.gd"
SpeedrunTimer="*res://eggs/speedrun_timer.gd"
PersistMan="*res://scripts/persist_man.gd"
